      "cell_type": "code",
      "source": [
        "reader = PdfReader(\"gnl-12-149.pdf?raw=true\")\n",
        "\n",
        "raw_text = \"\"\n",
        "\n",
        "for i, page in enumerate(reader.pages):\n",
        "    text = page.extract_text()\n",
        "    if text:\n",
        "        raw_text += text\n",
        "\n",
        "raw_text[:1000]"
